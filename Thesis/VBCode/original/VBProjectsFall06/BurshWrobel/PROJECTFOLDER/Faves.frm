Private Sub Command3_Click() 'Sorts the list by ascending order based upon input texts.
Dim Favorites(1 To 22) As String
Dim Inputs(1 To 22) As Integer
Dim CTR As Integer
Dim Temp As Integer
Dim Temp2 As String
Dim Pass As Integer
Open App.Path & "\Favorites.txt" For Input As #1
picResults.Cls

CTR = 1
Do While Not EOF(1)
    Input #1, Favorites(CTR)
    CTR = CTR + 1
Loop

    Inputs(1) = A.Text
    Inputs(2) = B.Text
    Inputs(3) = C.Text
    Inputs(4) = D.Text
    Inputs(5) = E.Text
    Inputs(6) = F.Text
    Inputs(7) = G.Text
    Inputs(8) = H.Text
    Inputs(9) = I.Text
    Inputs(10) = J.Text
    Inputs(11) = K.Text
    Inputs(12) = L.Text
    Inputs(13) = M.Text
    Inputs(14) = N.Text
    Inputs(15) = O.Text
    Inputs(16) = P.Text
    Inputs(17) = Q.Text
    Inputs(18) = R.Text
    Inputs(19) = S.Text
    Inputs(20) = T.Text
    Inputs(21) = U.Text
    Inputs(22) = V.Text
    

    For Pass = 1 To 21
    For CTR = 1 To 22 - Pass
        If Inputs(CTR) > Inputs(CTR + 1) Then
            Temp = Inputs(CTR)
            Inputs(CTR) = Inputs(CTR + 1)
            Inputs(CTR + 1) = Temp
            Temp2 = Favorites(CTR)
            Favorites(CTR) = Favorites(CTR + 1)
            Favorites(CTR + 1) = Temp2
            
        End If
    Next CTR
Next Pass
For CTR = 1 To 22
    picResults.Print Favorites(CTR), Tab(30); Inputs(CTR)
Next CTR
    picResults.Print
    picResults.Print "----------------------------Favorites----------------------------"
    picResults.Print "--------------------------- On Bottom ---------------------------"
Close #1
End Sub

