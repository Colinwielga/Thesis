Private Sub cmdRead_Click()
    Open App.Path & "\wordJFK.txt" For Append As #1
    d = InputBox("Enter a noun. (plural)")
    d1 = InputBox("Enter a noun.")
    d2 = InputBox("Enter a Celebrity's name.")
    d3 = InputBox("Enter a verb.")
    d4 = InputBox("Enter a city or state.")
    d5 = InputBox("Enter a nationality.")
    MsgBox ("For the next 10 inputs, make sure that the noun relates to the verb. Examples: swim in the pool or fish in the lake")
    d6 = InputBox("Enter a verb.")
    d7 = InputBox("Enter a noun relating to the last verb.")
    d8 = InputBox("Enter a verb.")
    d9 = InputBox("Enter a noun relating to the last verb.")
    d10 = InputBox("Enter a verb.")
    d11 = InputBox("Enter a noun relating to the last verb.")
    d12 = InputBox("Enter a verb.")
    d13 = InputBox("Enter a noun relating to the last verb.")
    d14 = InputBox("Enter a verb.")
    Write #1, d, d1, d2, d3, d4, d5, d6, d7, d8, d9, d10, d11, d12, d13, d14
    cmdDisplay.Enabled = True
    Close #1
    MsgBox ("Good Job, when you are ready to see your masterpiece, CLICK on Display Words.")
End Sub
